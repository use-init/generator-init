{
  "name": "<%= _.slugify(projectName) %>",
  "title": "",
  "description": "This is an initial boilerplate for front-end projects",
  "version": "1.0.2",
  "homepage": "https://github.com/use-init/init",
  <% if (githubUsername) { %>
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= githubUsername %>/<%= _.slugify(projectName) %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= _.slugify(projectName) %>/issues"
  },
  <% } %>
  "license": "MIT",
  "scripts": {
    "postinstall": "node_modules/.bin/bower install"
  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {},
  "devDependencies": {
    "bower": "~1.2.7",
    "matchdep": "0.3.0",
    "grunt": "0.4.x",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-connect": "0.5.0",
    "grunt-contrib-copy": "0.4.1",
    "grunt-contrib-imagemin": "~0.4.0",
    "grunt-contrib-jshint": "0.7.2",
    "grunt-contrib-requirejs": "0.4.1",
    <% if (cssPreprocessor === 'Sass') { %>
    "grunt-contrib-sass": "0.5.1",
    <% } %>
    <% if (cssPreprocessor === 'Compass') { %>
    "grunt-contrib-compass": "~0.7.0",
    <% } %>
    <% if (cssPreprocessor === 'Less') { %>
    "grunt-contrib-less": "~0.8.3",
    <% } %>
    "grunt-contrib-watch": "0.5.3",
    "grunt-modernizr": "0.4.1",
    "grunt-replace": "0.5.1",
    "grunt-karma": "~0.6.2",
    "karma-script-launcher": "~0.1.0",
    "karma-chrome-launcher": "~0.1.0",
    "karma-firefox-launcher": "~0.1.0",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.1.3",
    "karma-requirejs": "~0.2.0",
    "karma-phantomjs-launcher": "~0.1.0",
    "karma": "~0.10.2",
    "karma-safari-launcher": "~0.1.1",
    "karma-opera-launcher": "~0.1.0",
    "karma-coverage": "~0.1.0",
    "glob": "~3.2.6",
    "grunt-contrib-clean": "~0.5.0"
  },
  "keywords": []
}
